<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nucleussoftware.easy2learn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_finger_print"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_finger_print"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FlipActivity"></activity>
        <activity android:name=".QuestionsListActivity" />
        <activity android:name=".PrepareMaterialActivity" />
        <activity android:name=".ChooseActivity" />
        <activity android:name=".SubjectActivity" />
        <activity android:name=".AssessmentActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".QuestionPreview" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>